{
    "extends": ["react-app", "prettier"],
    "plugins": ["react", "prettier"],
    "rules": {
      "prettier/prettier": [
        "warn",
        {
            "arrowParens": "always",
            "semi": true,
            "singleQuote": true,
            "tabWidth": 2,
            "endOfLine": "auto",
            "useTabs": false,
            "jsxSingleQuote": false
        }
      ]
    }
}